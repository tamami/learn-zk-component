<?page title="Select Box" contentType="text/html;charset=UTF-8"?>
<zk>
<zscript><![CDATA[
  String[] buah = {"Pepaya", "Mangga", "Pisang", "Jambu"};
  ListModelList model = new ListModelList(buah);
  model.addToSelection("Mangga");
]]>
</zscript>
<window title="Select Box" border="normal">
  <selectbox model="${model}" onSelect="alert(model.get(event.getData()));">
    <template name="model">
      ${each }
    </template>
  </selectbox>
</window>
</zk>